{
  "name": "brunoroque06",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "audit": "lhci autorun",
    "build": "npm run pre && ng build",
    "deploy": "npm run build && gh-pages --branch website --dist dist/brunoroque06 --no-history --repo https://github.com/brunoroque06/brunoroque06.github.io.git",
    "format": "prettier --ignore-path .gitignore -w .",
    "format:check": "prettier --ignore-path .gitignore -l .",
    "pre": "node tools/main.js",
    "pre:watch": "nodemon --ext . --watch tools tools/main.js",
    "start": "npm run pre && ng serve",
    "watch": "npm run pre && ng build --watch --configuration development"
  },
  "dependencies": {
    "@angular/common": "17.0.0",
    "@angular/compiler": "17.0.0",
    "@angular/core": "17.0.0",
    "@angular/platform-browser": "17.0.0",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.0.0",
    "@angular/cli": "17.0.0",
    "@angular/compiler-cli": "17.0.0",
    "@lhci/cli": "0.12.0",
    "gh-pages": "6.0.0",
    "nodemon": "3.0.1",
    "playwright": "1.39.0",
    "prettier": "3.0.3",
    "sharp": "0.32.6",
    "typescript": "5.2.2"
  }
}